<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ThemeCamera" parent="@miui:style/V5.Theme.Dark">
        <item name="android:colorBackground">@*android:color/black</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:colorBackgroundCacheHint">@*android:color/black</item>
        <item name="miui:v5_bottom_bar_bg">?miui:v5_edit_mode_bottom_bar_bg</item>
        <item name="miui:v5_bottom_bar_icon_bg">?miui:v5_edit_mode_bottom_bar_icon_bg</item>
    </style>
    <style name="ThemeCameraSettings" parent="@miui:style/V5.Theme.Light.Settings">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="OnScreenHintTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*android:color/primary_text_dark</item>
        <item name="android:textColorHighlight">#ffff9200</item>
        <item name="android:textColorHint">#ff808080</item>
        <item name="android:textColorLink">#ff5c5cff</item>
    </style>
    <style name="OnScreenHintTextAppearance.Small" parent="@style/OnScreenHintTextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*android:color/secondary_text_dark</item>
    </style>
    <style name="Animation_OnScreenHint">
        <item name="android:windowEnterAnimation">@anim/on_screen_hint_enter</item>
        <item name="android:windowExitAnimation">@anim/on_screen_hint_exit</item>
    </style>
    <style name="ReviewControlIcon">
        <item name="android:gravity">center</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="ReviewPlayIcon">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/ic_gallery_play_big</item>
        <item name="android:layout_centerInParent">true</item>
    </style>
    <style name="SettingPopupWindow">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="PopupTitleText">
        <item name="android:textSize">@dimen/popup_title_text_size</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/popup_title_color</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">left|center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/popup_title_height</item>
        <item name="android:layout_marginLeft">18.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">@color/popup_title_shadow_color</item>
        <item name="android:shadowDy">4.0</item>
        <item name="android:shadowRadius">10.0</item>
    </style>
    <style name="PopupTitleSeperator">
        <item name="android:background">@color/popup_title_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/popup_seperator_height</item>
        <item name="android:layout_marginLeft">@dimen/popup_listview_left_padding</item>
        <item name="android:layout_marginTop">4.0dip</item>
        <item name="android:layout_marginRight">@dimen/popup_listview_left_padding</item>
        <item name="android:layout_marginBottom">20.0dip</item>
    </style>
    <style name="SettingItemList">
        <item name="android:layout_gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:scrollbars">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/popup_listview_left_padding</item>
        <item name="android:layout_marginRight">@dimen/popup_listview_left_padding</item>
        <item name="android:cacheColorHint">@null</item>
    </style>
    <style name="SettingItemRulerLine">
        <item name="android:layout_gravity">center</item>
        <item name="android:scrollbars">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:cacheColorHint">@null</item>
    </style>
    <style name="SettingItemRulerPoints">
        <item name="android:layout_gravity">center</item>
        <item name="android:scrollbars">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/popup_ruler_point_left_padding</item>
        <item name="android:layout_marginRight">@dimen/popup_ruler_point_left_padding</item>
        <item name="android:cacheColorHint">@null</item>
    </style>
    <style name="SettingItemListHorizontal">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:scrollbars">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/popup_listview_left_padding</item>
        <item name="android:layout_marginRight">@dimen/popup_listview_left_padding</item>
        <item name="android:layout_marginBottom">@dimen/popup_ruler_point_left_padding</item>
        <item name="android:cacheColorHint">@null</item>
    </style>
    <style name="SettingItemGrid">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">15.0dip</item>
        <item name="android:layout_marginBottom">15.0dip</item>
        <item name="android:listSelector">@*android:color/transparent</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:horizontalSpacing">5.0dip</item>
        <item name="android:verticalSpacing">5.0dip</item>
        <item name="android:numColumns">4</item>
    </style>
    <style name="SettingSimpleModeGrid">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:numColumns">3</item>
    </style>
    <style name="SettingItemTitle">
        <item name="android:textSize">@dimen/setting_item_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">start|center</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="SettingItemTitleColumn">
        <item name="android:textSize">@dimen/setting_item_horizontal_text_size</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="SettingItemText">
        <item name="android:textSize">@dimen/setting_item_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">@dimen/setting_item_text_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="SettingRow">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/setting_picker</item>
        <item name="android:paddingLeft">@dimen/setting_item_list_margin</item>
        <item name="android:paddingRight">@dimen/setting_item_list_margin</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/setting_row_height</item>
    </style>
    <style name="SettingColumn">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SettingKnob">
        <item name="android:textSize">@dimen/setting_knob_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@drawable/btn_setting_picker</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@*android:color/transparent</item>
        <item name="android:layout_width">@dimen/setting_knob_width</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="OnScreenIndicator">
        <item name="android:paddingLeft">1.0dip</item>
        <item name="android:paddingRight">1.0dip</item>
        <item name="android:focusable">false</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="ScrollButtonImageView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="RotateImageViewStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="OnScreenIndicators">
        <item name="android:background">@drawable/bg_status_bar</item>
        <item name="android:paddingLeft">26.5dip</item>
        <item name="android:paddingRight">26.5dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="PanoViewHorizontalBar">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.5</item>
        <item name="android:alpha">1.0</item>
    </style>
    <style name="ReviewThumbnail">
        <item name="android:layout_width">@dimen/thumbnail_size</item>
        <item name="android:layout_height">@dimen/thumbnail_size</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="ReviewThumbnailMask">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="PanoCustomDialogText">
        <item name="android:textAppearance">@*android:style/TextAppearance.Medium</item>
    </style>
    <style name="EffectSettingGrid">
        <item name="android:paddingBottom">3.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/setting_item_list_margin</item>
        <item name="android:layout_marginRight">@dimen/setting_item_list_margin</item>
        <item name="android:horizontalSpacing">3.0dip</item>
        <item name="android:verticalSpacing">3.0dip</item>
        <item name="android:numColumns">3</item>
        <item name="android:choiceMode">singleChoice</item>
    </style>
    <style name="EffectSettingItem">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/setting_picker</item>
        <item name="android:paddingLeft">2.0dip</item>
        <item name="android:paddingTop">9.0dip</item>
        <item name="android:paddingRight">2.0dip</item>
        <item name="android:paddingBottom">9.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="EffectSettingItemTitle">
        <item name="android:textSize">@dimen/effect_setting_item_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">1.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="WhiteBalanceSettingItemText">
        <item name="android:textSize">@dimen/grid_setting_item_textSize</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">@dimen/grid_setting_item_text_paddingBottom</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="EffectSettingTypeTitle">
        <item name="android:textSize">@dimen/effect_setting_type_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">start|center</item>
        <item name="android:paddingLeft">@dimen/effect_setting_type_text_left_padding</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/effect_setting_type_text_min_height</item>
        <item name="android:singleLine">true</item>
        <item name="android:alpha">0.7</item>
    </style>
    <style name="EffectTypeSeparator">
        <item name="android:background">#ff2c2c2c</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0dip</item>
        <item name="android:layout_marginLeft">8.0dip</item>
        <item name="android:layout_marginRight">8.0dip</item>
        <item name="android:layout_marginBottom">14.0dip</item>
    </style>
    <style name="EffectTitleSeparator">
        <item name="android:background">@*android:drawable/divider_horizontal_dark</item>
        <item name="android:paddingBottom">4.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0dip</item>
    </style>
    <style name="OneRowGrid">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:stretchMode">none</item>
    </style>
    <style name="ViewfinderLabelLayout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">13.0dip</item>
        <item name="android:layout_marginTop">13.0dip</item>
        <item name="android:layout_marginRight">13.0dip</item>
        <item name="android:layout_marginBottom">@dimen/indicator_bar_width</item>
    </style>
    <style name="Theme.Gallery" parent="@miui:style/V5.Theme.Dark">
        <item name="android:colorBackground">@null</item>
        <item name="android:windowBackground">@*android:color/black</item>
        <item name="android:windowNoTitle">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowActionBar">true</item>
        <item name="android:displayOptions"></item>
        <item name="miui:v5_bottom_bar_bg">?miui:v5_edit_mode_bottom_bar_bg</item>
        <item name="miui:v5_bottom_bar_icon_bg">?miui:v5_edit_mode_bottom_bar_icon_bg</item>
    </style>
    <style name="MediaButton.Play" parent="@*android:style/MediaButton.Play">
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/icn_media_play</item>
    </style>
    <style name="DialogPickerTheme" parent="@style/Theme.Gallery" />
</resources>
