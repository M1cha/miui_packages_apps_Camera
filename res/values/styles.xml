<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ThemeCamera" parent="@miui:style/Theme.Dark">
        <item name="android:colorBackground">@android:color/black</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="miui:windowActionBar">false</item>
    </style>
    <style name="ThemeCameraSettings" parent="@miui:style/Theme.Light.Settings" />
    <style name="OnScreenHintTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/primary_text_dark</item>
        <item name="android:textColorHighlight">#ffff9200</item>
        <item name="android:textColorHint">#ff808080</item>
        <item name="android:textColorLink">#ff5c5cff</item>
    </style>
    <style name="OnScreenHintTextAppearance.Small" parent="@style/OnScreenHintTextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/secondary_text_dark</item>
    </style>
    <style name="Animation_OnScreenHint">
        <item name="android:windowEnterAnimation">@anim/on_screen_hint_enter</item>
        <item name="android:windowExitAnimation">@anim/on_screen_hint_exit</item>
    </style>
    <style name="ReviewControlIcon">
        <item name="android:gravity">center</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="ReviewPlayIcon">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/ic_gallery_play_big</item>
        <item name="android:layout_centerInParent">true</item>
    </style>
    <style name="SettingPopupWindow">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="PopupTitleSeperator">
        <item name="android:background">@color/popup_title_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/popup_seperator_height</item>
        <item name="android:layout_marginLeft">@dimen/popup_listview_left_padding</item>
        <item name="android:layout_marginTop">4.0dip</item>
        <item name="android:layout_marginRight">@dimen/popup_listview_left_padding</item>
        <item name="android:layout_marginBottom">20.0dip</item>
    </style>
    <style name="PopupSeparator">
        <item name="android:background">#33ffffff</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/popup_seperator_height</item>
    </style>
    <style name="SettingItemRulerLine">
        <item name="android:layout_gravity">center</item>
        <item name="android:scrollbars">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:cacheColorHint">@null</item>
    </style>
    <style name="SettingItemGrid">
        <item name="android:layout_gravity">center</item>
        <item name="android:scrollbars">none</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">15.0dip</item>
        <item name="android:layout_marginBottom">15.0dip</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:horizontalSpacing">5.0dip</item>
        <item name="android:verticalSpacing">5.0dip</item>
        <item name="android:numColumns">5</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <style name="SettingExpandedTextGrid">
        <item name="android:scrollbars">none</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:horizontalSpacing">0.0dip</item>
        <item name="android:verticalSpacing">0.0dip</item>
        <item name="android:numColumns">5</item>
        <item name="android:choiceMode">singleChoice</item>
        <item name="android:layoutDirection">locale</item>
    </style>
    <style name="SettingItemTitle">
        <item name="android:textSize">@dimen/setting_item_text_size</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="SettingColumn">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SettingStatusBarImage">
        <item name="android:gravity">center</item>
        <item name="android:focusable">false</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">1.0dip</item>
        <item name="android:layout_marginBottom">1.0dip</item>
        <item name="android:scaleType">center</item>
        <item name="android:alpha">0.6</item>
    </style>
    <style name="SettingStatusBarText">
        <item name="android:textSize">@dimen/v6_setting_status_bar_textSize</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:focusable">false</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">@dimen/v6_setting_status_bar_textWidth</item>
        <item name="android:layout_height">@dimen/v6_setting_status_bar_textWidth</item>
        <item name="android:layout_marginTop">@dimen/v6_setting_status_bar_margin_top</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:shadowColor">#c0000000</item>
        <item name="android:shadowRadius">4.0</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
    <style name="ScrollButtonImageView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="PanelButtonStyle">
        <item name="android:paddingLeft">@dimen/panel_imageview_button_padding_width</item>
        <item name="android:paddingRight">@dimen/panel_imageview_button_padding_width</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="PanelRotateImageViewStyle" parent="@style/PanelButtonStyle">
        <item name="android:scaleType">center</item>
    </style>
    <style name="PanelTextButtonStyle" parent="@style/PanelButtonStyle">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="RotateImageViewStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="OnScreenIndicators">
        <item name="android:background">@drawable/bg_status_bar</item>
        <item name="android:paddingLeft">26.5dip</item>
        <item name="android:paddingRight">40.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="PanoViewHorizontalBar">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.5</item>
        <item name="android:alpha">1.0</item>
    </style>
    <style name="ReviewThumbnail">
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">1.0dip</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="ReviewThumbnailMask">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="PanoCustomDialogText">
        <item name="android:textAppearance">@android:style/TextAppearance.Medium</item>
    </style>
    <style name="EffectItemText">
        <item name="android:textSize">@dimen/effect_item_text_size</item>
        <item name="android:textColor">@color/effect_item_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="EffectToastText">
        <item name="android:textSize">@dimen/effect_toast_text_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:shadowColor">#c0000000</item>
        <item name="android:shadowRadius">4.0</item>
    </style>
    <style name="GridSettingItemText">
        <item name="android:textSize">@dimen/grid_setting_item_textSize</item>
        <item name="android:textColor">@color/grid_setting_item_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="GridTextOnlySettingItem">
        <item name="android:textSize">@dimen/grid_text_only_setting_item_textSize</item>
        <item name="android:textColor">@color/v6_grid_item_highlight_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="SingeTextItemTextStyle">
        <item name="android:textSize">@dimen/grid_text_only_setting_item_textSize</item>
        <item name="android:textColor">@color/text_item_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="GridExpandedTextSettingItem" parent="@style/GridSettingItemText">
        <item name="android:textSize">@dimen/expanded_text_item_textSize</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="RecordingTimeStyle">
        <item name="android:textSize">@dimen/v6_recording_time_textSize</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowRadius">4.0</item>
        <item name="android:drawableLeft">@drawable/v6_ic_video_recordtime_indicator</item>
        <item name="android:drawablePadding">5.0dip</item>
    </style>
    <style name="EffectTypeSeparator">
        <item name="android:background">#ff2c2c2c</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0dip</item>
        <item name="android:layout_marginLeft">8.0dip</item>
        <item name="android:layout_marginRight">8.0dip</item>
        <item name="android:layout_marginBottom">14.0dip</item>
    </style>
    <style name="EffectTitleSeparator">
        <item name="android:background">@android:drawable/divider_horizontal_dark</item>
        <item name="android:paddingBottom">4.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0dip</item>
    </style>
    <style name="OneRowGrid">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:stretchMode">none</item>
    </style>
    <style name="V6LayoutTopStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/v6_setting_top_view_height</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
    <style name="V6LayoutBottomStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="V6SettingTopTextAppearance">
        <item name="android:textSize">@dimen/v6_setting_top_textSize</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">#e6ffffff</item>
    </style>
    <style name="DebugTextStyle">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ffff0000</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">2.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="RemindStyle">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/ic_new_remind</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginStart">6.0dip</item>
    </style>
    <style name="SkinBeautifySeekbarTextStyle">
        <item name="android:textSize">@dimen/skin_beatify_seekbar_textSize</item>
        <item name="android:textColor">@color/skin_beatify_seekbar_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="SnapTipTextStyle">
        <item name="android:textSize">@dimen/snap_tip_text_size</item>
        <item name="android:textColor">@color/white_alpha_cc</item>
    </style>
    <style name="ZoomButtonDigitsTextStyle">
        <item name="android:textSize">@dimen/zoom_button_digits_textSize</item>
        <item name="android:textColor">@color/white_alpha_cc</item>
    </style>
    <style name="ZoomButtonXTextStyle">
        <item name="android:textSize">@dimen/zoom_button_x_textSize</item>
        <item name="android:textColor">@color/white_alpha_cc</item>
    </style>
    <style name="ZoomPopupDigitsTextStyle">
        <item name="android:textSize">@dimen/zoom_popup_digits_textSize</item>
        <item name="android:textColor">@color/zoom_popup_text_item_text_color</item>
    </style>
    <style name="ZoomPopupXTextStyle">
        <item name="android:textSize">@dimen/zoom_popup_x_textSize</item>
        <item name="android:textColor">@color/zoom_popup_text_item_text_color</item>
    </style>
    <style name="PortraitHintTextStyle">
        <item name="android:textSize">@dimen/portrait_hint_textSize</item>
        <item name="android:textColor">@color/portrait_hint_text_color</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="BeautyCameraEffectRadioButton">
        <item name="android:textSize">@dimen/beautycamera_tab_size</item>
        <item name="android:textColor">@color/beauty_tab_selector_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">@dimen/beautycamera_effect_tab_margin</item>
        <item name="android:paddingRight">@dimen/beautycamera_effect_tab_margin</item>
        <item name="android:button">@null</item>
        <item name="android:minWidth">@dimen/beautycamera_effect_tab_width</item>
    </style>
    <style name="BeautyCameraBeautyLevelRadioButton">
        <item name="android:layout_marginLeft">@dimen/beautycamera_beauty_level_radio_button_margin_horizontal</item>
        <item name="android:layout_marginRight">@dimen/beautycamera_beauty_level_radio_button_margin_horizontal</item>
        <item name="android:scaleX">0.7</item>
        <item name="android:scaleY">0.7</item>
    </style>
    <style name="PopupButtonIndicator">
        <item name="android:background">@drawable/bg_popup_indicator</item>
        <item name="android:layout_width">@dimen/manually_indicator_layout_size</item>
        <item name="android:layout_height">@dimen/manually_indicator_layout_size</item>
    </style>
    <style name="PortraitUseHintTitleStyle">
        <item name="android:textSize">@dimen/portrait_use_hint_title_textSize</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="PortraitUseHintContentStyle">
        <item name="android:textSize">@dimen/portrait_use_hint_content_textSize</item>
        <item name="android:textColor">#99ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:lineSpacingExtra">@dimen/portrait_use_hint_content_text_spacing</item>
    </style>
    <style name="DialogFragmentFullScreen" parent="@android:style/Theme.DeviceDefault.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
</resources>
